# Docker Environment Configuration for Open SWE
# Copy this file to .env and fill in your values

# ------------------LangSmith tracing------------------
LANGCHAIN_PROJECT=default
LANGCHAIN_API_KEY=
LANGCHAIN_TRACING_V2=true
LANGCHAIN_TEST_TRACKING=false

# ------------------LLM Provider Keys------------------
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_API_KEY=

# ------------------Infrastructure---------------------
# Daytona API key for creating & accessing the cloud sandbox
DAYTONA_API_KEY=

# ------------------------Tools------------------------
# Firecrawl API key for calling the get URL contents tool
FIRECRAWL_API_KEY=

# ------------------Github App Secrets-----------------
GITHUB_APP_NAME=
GITHUB_APP_ID=
# App secret key (should be multi-line in actual .env)
GITHUB_APP_PRIVATE_KEY=
# Secret key for verifying GitHub webhook events
GITHUB_WEBHOOK_SECRET=
# GitHub username to tag for triggering runs from PR comments
GITHUB_TRIGGER_USERNAME=open-swe

# ------------------------Other------------------------
# URL of the web app (use http://localhost:3000 for local, or your domain for production)
OPEN_SWE_APP_URL=http://localhost:3000

# Encryption key for secrets (32-byte hex string for AES-256)
# Generate with: openssl rand -hex 32
SECRETS_ENCRYPTION_KEY=

# Whether to append "[skip ci]" to commit messages
SKIP_CI_UNTIL_LAST_COMMIT=true

# Agent URL for web app (use http://agent:2024 for Docker network)
NEXT_PUBLIC_AGENT_URL=http://agent:2024

# List of GitHub usernames allowed to use Open SWE
NEXT_PUBLIC_ALLOWED_USERS_LIST=[]
